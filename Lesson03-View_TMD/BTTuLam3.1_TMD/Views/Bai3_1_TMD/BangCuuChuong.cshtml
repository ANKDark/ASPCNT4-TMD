@{
    ViewBag.Title = "BangCuuChuong";
}

<h2>Bảng cửu chương</h2>

@{
    for (int i = 2; i <= 9; i++)
    {
        <p>Bảng cửu chương của @i</p>
        for (int j = 1; j <= 10; j++)
        {
            int pheptinh = i * j;
            <p>@i x @j = @pheptinh</p>
        }
        <br />
    }
}
