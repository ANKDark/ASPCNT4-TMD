@model IEnumerable<TMDLesson06.Models.TMDSong>

@{
    ViewBag.Title = "Danh sách bài hát";
}

<h2>Danh sách bài hát</h2>

<p>
    @Html.ActionLink("Create New", "TMDCreate")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TMDTitle)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TMDAuther)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TMDArtist)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TMDYearRelease)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TMDTitle)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TMDAuther)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TMDArtist)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TMDYearRelease)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
