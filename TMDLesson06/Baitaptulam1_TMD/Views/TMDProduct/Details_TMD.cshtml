@model Baitaptulam1_TMD.Models.Product_TMD

@{
    ViewBag.Title = "Details_TMD";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Details_TMD</h2>

<div>
    <h3>Thông tin chi tiết</h3>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Mã sản phẩm
        </dt>
        <dd>
            @Html.DisplayFor(model => model.ProductId_TMD)
        </dd>
        <dt>
            Tên sản phẩm
        </dt>
        <dd>
            @Html.DisplayFor(model => model.ProductName_TMD)
        </dd>
        <dt>
            Đơn vị
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Unit_TMD)
        </dd>
        <dt>
            Giá
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Price_TMD)
        </dd>
    </dl>
</div>
<p>
    @Html.ActionLink("Sửa", "Edit_TMD", new { id = Model.ProductId_TMD })
    @Html.ActionLink("Quay lại", "GetProduct_TMD")
</p>
